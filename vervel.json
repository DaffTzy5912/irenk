{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/generate",
      "dest": "/generate"
    },
    {
      "src": "/apply-effect",
      "dest": "/apply-effect"
    },
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ]
}
